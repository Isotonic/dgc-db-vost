#Env file used to pass to Docker.

#Postgres Settings.
#Postgres server URL.
POSTGRES_URL=
#Postgres username.
POSTGRES_USER=
#Postgres DB name.
POSTGRES_DB=
#Postgres user password.
POSTGRES_PASSWORD=

#Secret Keys.
#Secret key to be used for Flask, make sure it's secure. Generate one.
FLASK_SECRET_KEY=
#Secret key to be used for JWT, make sure it's secure. Generate one.
JWT_SECRET_KEY=

#Email Settings.
#Mail server URL.
MAIL_SERVER=
#Mail server port.
MAIL_PORT=
#True/False to use TLS or not, default is false.
MAIL_USE_TLS=False
#True/False to use SSL or not, default is true.
MAIL_USE_SSL=True
#Username for mail server account.
MAIL_USERNAME=
#Password for mail server account.
MAIL_PASSWORD=

#Vue Map's.
#Generate one from this URL and restrict it to your website only. https://www.mapbox.com/
VUE_APP_MAPBOX_API_KEY=
#Name of the map tile server, e.g Wikimedia Maps
VUE_APP_MAP_TILE_SERVER_NAME=
#Url to the tile server without the $, e.g https://maps.wikimedia.org/osm-intl/{z}/{x}/{y}.png
VUE_APP_MAP_TILE_SERVER_Url=
#Link to redirect users to if they click the attribution at the bottom right of the map, e.g https://foundation.wikimedia.org/wiki/Maps_Terms_of_Use
VUE_APP_MAP_TILE_SERVER_Link=

#Misc.
#Website domain name. Example, dgvost.co.uk
DOMAIN_NAME=
#Vue port.
PORT=8765